<%= form_for(:session, url: login_path) do |f| %>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email, autocomplete: :off %>
  </div>
  <div class="field">
    <%= f.label "Password" %><br>
    <%= f.text_field :password, type: :password %>
  </div>
  <div class="actions">
    <%= f.submit "Login" %>
  </div>
<% end %>

<%= link_to("Register", new_user_path) %>