<%= form_for(:session) do |f| %>

  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email, autocomplete: :off %>
  </div>
  <div class="field">
    <%= f.label "Password" %><br>
    <%= f.text_field :password_digest, type: :password %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>